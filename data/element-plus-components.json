          },
          {
            "name": "file",
            "type": "object",
            "description": "上传成功的文件"
          },
          {
            "name": "fileList",
            "type": "array",
            "description": "全部文件列表"
          }
        ]
      },
      {
        "name": "error",
        "description": "文件上传失败时触发",
        "params": [
          {
            "name": "error",
            "type": "Error",
            "description": "错误对象"
          },
          {
            "name": "file",
            "type": "object",
            "description": "上传失败的文件"
          },
          {
            "name": "fileList",
            "type": "array",
            "description": "全部文件列表"
          }
        ]
      }
    ]
  },
