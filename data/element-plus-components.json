[
  {
    "name": "ElButton",
    "description": "按钮组件，用于触发用户操作",
    "category": "基础组件",
    "props": [
      {
        "name": "size",
        "type": "string",
        "description": "按钮大小",
        "default": "default",
        "required": false
      },
      {
        "name": "type",
        "type": "string",
        "description": "按钮类型",
        "default": "default",
        "required": false
      },
      {
        "name": "plain",
        "type": "boolean",
        "description": "是否朴素按钮",
        "default": "false",
        "required": false
      },
      {
        "name": "round",
        "type": "boolean",
        "description": "是否圆角按钮",
        "default": "false",
        "required": false
      },
      {
        "name": "disabled",
        "type": "boolean",
        "description": "是否禁用状态",
        "default": "false",
        "required": false
      }
    ],
    "events": [
      {
        "name": "click",
        "description": "点击按钮时触发",
        "params": [
          {
            "name": "event",
            "type": "MouseEvent",
            "description": "原生事件对象"
          }
        ]
      }
    ],
    "slots": [
      {
        "name": "default",
        "description": "按钮内容"
      },
      {
        "name": "loading",
        "description": "自定义加载图标内容"
      },
      {
        "name": "icon",
        "description": "自定义图标"
      }
    ]
  },
  {
    "name": "ElTable",
    "description": "表格组件，用于展示多条结构类似的数据",
    "category": "数据展示",
    "props": [
      {
        "name": "data",
        "type": "array",
        "description": "表格数据",
        "required": true
      },
      {
        "name": "height",
        "type": "string/number",
        "description": "表格高度",
        "required": false
      },
      {
        "name": "border",
        "type": "boolean",
        "description": "是否带有纵向边框",
        "default": "false",
        "required": false
      },
      {
        "name": "stripe",
        "type": "boolean",
        "description": "是否斑马纹表格",
        "default": "false",
        "required": false
      }
    ],
    "events": [
      {
        "name": "select",
        "description": "当用户手动勾选数据行时触发",
        "params": [
          {
            "name": "selection",
            "type": "array",
            "description": "当前选中的所有数据"
          },
          {
            "name": "row",
            "type": "object",
            "description": "当前选中的行数据"
          }
        ]
      },
      {
        "name": "sort-change",
        "description": "当表格的排序条件发生变化时触发",
        "params": [
          {
            "name": "column",
            "type": "object",
            "description": "当前列的配置对象"
          },
          {
            "name": "prop",
            "type": "string",
            "description": "当前列的字段名"
          },
          {
            "name": "order",
            "type": "string",
            "description": "排序规则"
          }
        ]
      }
    ]
  },
  {
    "name": "ElForm",
    "description": "表单组件，用于收集、验证和提交用户输入的数据",
    "category": "数据录入",
    "props": [
      {
        "name": "model",
        "type": "object",
        "description": "表单数据对象",
        "required": true
      },
      {
        "name": "rules",
        "type": "object",
        "description": "表单验证规则",
        "required": false
      },
      {
        "name": "label-width",
        "type": "string | number",
        "description": "标签宽度",
        "default": "auto",
        "required": false
      },
      {
        "name": "disabled",
        "type": "boolean",
        "description": "是否禁用该表单内的所有组件",
        "default": "false",
        "required": false
      }
    ],
    "events": [
      {
        "name": "validate",
        "description": "任一表单项被校验后触发",
        "params": [
          {
            "name": "prop",
            "type": "string",
            "description": "被校验的表单项 prop"
          },
          {
            "name": "isValid",
            "type": "boolean",
            "description": "是否校验通过"
          },
          {
            "name": "message",
            "type": "string",
            "description": "校验信息"
          }
        ]
      }
    ],
    "slots": [
      {
        "name": "default",
        "description": "表单项内容"
      }
    ]
  },
  {
    "name": "ElFormItem",
    "description": "表单项组件，用于表单字段的布局和验证",
    "category": "表单组件",
    "props": [
      {
        "name": "prop",
        "type": "string",
        "description": "表单域 model 字段",
        "required": false
      },
      {
        "name": "label",
        "type": "string",
        "description": "标签文本",
        "required": false
      },
      {
        "name": "label-width",
        "type": "string | number",
        "description": "标签宽度",
        "required": false
      },
      {
        "name": "required",
        "type": "boolean",
        "description": "是否必填",
        "default": "false",
        "required": false
      }
    ],
    "slots": [
      {
        "name": "default",
        "description": "表单控件内容"
      },
      {
        "name": "label",
        "description": "标签内容"
      },
      {
        "name": "error",
        "description": "自定义错误信息"
      }
    ]
  },
  {
