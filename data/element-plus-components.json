    ],
    "slots": [
      {
        "name": "default",
        "description": "Dialog 的内容"
      },
      {
        "name": "header",
        "description": "Dialog 标题区的内容"
      },
      {
        "name": "footer",
        "description": "Dialog 按钮操作区的内容"
      }
    ]
  },
