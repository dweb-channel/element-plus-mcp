  {
    "name": "ElUpload",
    "description": "上传组件，用于文件上传",
    "category": "数据录入",
    "props": [
      {
        "name": "action",
        "type": "string",
        "description": "上传的URL",
        "required": true
      },
      {
        "name": "headers",
        "type": "object",
        "description": "设置上传的请求头部",
        "required": false
      },
