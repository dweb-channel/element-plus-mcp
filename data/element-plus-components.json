  {
    "name": "ElSwitch",
    "description": "开关组件",
    "category": "表单组件",
    "props": [
      {
        "name": "modelValue / v-model",
        "type": "boolean | string | number",
        "description": "绑定值",
        "required": false
      },
      {
        "name": "disabled",
        "type": "boolean",
        "description": "是否禁用",
        "default": "false",
